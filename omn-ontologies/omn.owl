    <!-- http://open-multinet.info/ontology/omn#adaptableFrom -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#adaptableFrom">
        <rdfs:label xml:lang="en">adaptable from</rdfs:label>
        <rdfs:comment xml:lang="en">determines the resource from which this resource can be adapted from - e.g. from an Ethernet to a FDDI port.</rdfs:comment>
        <rdfs:domain rdf:resource="http://open-multinet.info/ontology/omn#Layer"/>
        <rdfs:range rdf:resource="http://open-multinet.info/ontology/omn#Layer"/>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#adaptableTo -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#adaptableTo">
        <rdfs:label xml:lang="en">adaptable to</rdfs:label>
        <rdfs:comment xml:lang="en">determines to which resource this resource can adapts to - e.g. from an Ethernet to a FDDI port.</rdfs:comment>
        <rdfs:domain rdf:resource="http://open-multinet.info/ontology/omn#Layer"/>
        <rdfs:range rdf:resource="http://open-multinet.info/ontology/omn#Layer"/>
        <owl:inverseOf rdf:resource="http://open-multinet.info/ontology/omn#adaptableFrom"/>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#adaptsFrom -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#adaptsFrom">
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdfs:label xml:lang="en">adapts from</rdfs:label>
        <rdfs:comment xml:lang="en">determines from which resource this resource adapts - e.g. from an Ethernet to a FDDI port.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://open-multinet.info/ontology/omn#adaptsTo"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Component"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Group"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Component"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Group"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#adaptsTo -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#adaptsTo">
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdfs:label xml:lang="en">adapts to</rdfs:label>
        <rdfs:comment xml:lang="en">determines to which resource this resource adapts - e.g. from an Ethernet to a FDDI port.</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Component"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Group"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Component"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Group"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#dependsOn -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#dependsOn">
        <rdfs:label xml:lang="en">depends on</rdfs:label>
        <rdfs:comment xml:lang="en">claims dependency</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://open-multinet.info/ontology/omn#relatesTo"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Component"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Group"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Component"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Group"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#fromDependency -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#fromDependency">
        <rdfs:label xml:lang="en">from dependency</rdfs:label>
        <rdfs:comment xml:lang="en">claims dependency</rdfs:comment>
        <rdfs:domain rdf:resource="http://open-multinet.info/ontology/omn#Dependency"/>
        <owl:inverseOf rdf:resource="http://open-multinet.info/ontology/omn#toDependency"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Component"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Group"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#hasAttribute -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#hasAttribute">
        <rdfs:label xml:lang="en">has attribute</rdfs:label>
        <rdfs:comment xml:lang="en">link to a general attribute of the resource - e.g. to a ReadOnly class</rdfs:comment>
        <rdfs:range rdf:resource="http://open-multinet.info/ontology/omn#Attribute"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Component"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Group"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#hasComponent -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#hasComponent">
        <rdfs:label xml:lang="en">has component</rdfs:label>
        <rdfs:comment xml:lang="en">component of the resource - e.g. a CPU</rdfs:comment>
        <rdfs:seeAlso rdf:resource="&novi;hasComponent"/>
        <rdfs:range rdf:resource="http://open-multinet.info/ontology/omn#Component"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Component"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#hasGroup -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#hasGroup">
        <rdfs:label xml:lang="en">has group</rdfs:label>
        <rdfs:comment xml:lang="en">a group that is related to this resource - e.g. a reserved topology within an infrastructure</rdfs:comment>
        <rdfs:range rdf:resource="http://open-multinet.info/ontology/omn#Group"/>
        <rdfs:domain rdf:resource="http://open-multinet.info/ontology/omn#Group"/>
        <owl:inverseOf rdf:resource="http://open-multinet.info/ontology/omn#isGroupOf"/>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#hasReservation -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#hasReservation">
        <rdfs:label xml:lang="en">has reservation</rdfs:label>
        <rdfs:comment xml:lang="en">the reservation details of a resource - e.g. an immediate reservation for 3 hours</rdfs:comment>
        <rdfs:range rdf:resource="http://open-multinet.info/ontology/omn#Reservation"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Group"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#hasResource -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#hasResource">
        <rdfs:label xml:lang="en">has resource</rdfs:label>
        <rdfs:comment xml:lang="en">a resource that this resource contains - e.g. a node within a reserved topology</rdfs:comment>
        <rdfs:seeAlso rdf:resource="&novi;contains"/>
        <rdfs:domain rdf:resource="http://open-multinet.info/ontology/omn#Group"/>
        <rdfs:range rdf:resource="http://open-multinet.info/ontology/omn#Resource"/>
        <owl:inverseOf rdf:resource="http://open-multinet.info/ontology/omn#isResourceOf"/>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#hasService -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#hasService">
        <rdfs:label xml:lang="en">has service</rdfs:label>
        <rdfs:comment xml:lang="en">a service that this resource contains - e.g. a Hadoop instance within a reserved topology</rdfs:comment>
        <rdfs:range rdf:resource="http://open-multinet.info/ontology/omn#Service"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Group"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#isAttributeOf -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#isAttributeOf">
        <rdfs:label xml:lang="en">is attribute of</rdfs:label>
        <rdfs:comment xml:lang="en">a general attribute of a resource - e.g. to a ReadOnly class</rdfs:comment>
        <rdfs:domain rdf:resource="http://open-multinet.info/ontology/omn#Attribute"/>
        <owl:inverseOf rdf:resource="http://open-multinet.info/ontology/omn#hasAttribute"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Component"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Group"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#isComponentOf -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#isComponentOf">
        <rdfs:label xml:lang="en">is component of</rdfs:label>
        <rdfs:comment xml:lang="en">is component of a resource - e.g. a CPU in a PC</rdfs:comment>
        <rdfs:domain rdf:resource="http://open-multinet.info/ontology/omn#Component"/>
        <owl:inverseOf rdf:resource="http://open-multinet.info/ontology/omn#hasComponent"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Component"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#isGroupOf -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#isGroupOf">
        <rdfs:label xml:lang="en">is group of</rdfs:label>
        <rdfs:comment xml:lang="en">a group that is related to a resource - e.g. a reserved topology within an infrastructure</rdfs:comment>
        <rdfs:domain rdf:resource="http://open-multinet.info/ontology/omn#Group"/>
        <rdfs:range rdf:resource="http://open-multinet.info/ontology/omn#Group"/>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#isReservationOf -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#isReservationOf">
        <rdfs:label xml:lang="en">is reservation of</rdfs:label>
        <rdfs:comment xml:lang="en">the reservation details of a resource - e.g. an immediate reservation for 3 hours</rdfs:comment>
        <rdfs:domain rdf:resource="http://open-multinet.info/ontology/omn#Reservation"/>
        <owl:inverseOf rdf:resource="http://open-multinet.info/ontology/omn#hasReservation"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Group"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#isResourceOf -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#isResourceOf">
        <rdfs:label xml:lang="en">is resource of</rdfs:label>
        <rdfs:comment xml:lang="en">a resource that another resource contains - e.g. a node within a reserved topology</rdfs:comment>
        <rdfs:seeAlso rdf:resource="&novi;contains"/>
        <rdfs:range rdf:resource="http://open-multinet.info/ontology/omn#Group"/>
        <rdfs:domain rdf:resource="http://open-multinet.info/ontology/omn#Resource"/>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#isServiceOf -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#isServiceOf">
        <rdfs:label xml:lang="en">is service of</rdfs:label>
        <rdfs:comment xml:lang="en">a service of a resource - e.g. a Hadoop instance within a reserved topology</rdfs:comment>
        <rdfs:domain rdf:resource="http://open-multinet.info/ontology/omn#Service"/>
        <owl:inverseOf rdf:resource="http://open-multinet.info/ontology/omn#hasService"/>
        <rdfs:seeAlso rdf:resource="&service;providedBy"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Group"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://open-multinet.info/ontology/omn#relatesTo -->

    <owl:ObjectProperty rdf:about="http://open-multinet.info/ontology/omn#relatesTo">
        <rdfs:label xml:lang="en">relates to</rdfs:label>
        <rdfs:comment xml:lang="en">claims a general dependency</rdfs:comment>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Component"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Group"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Resource"/>
                    <rdf:Description rdf:about="http://open-multinet.info/ontology/omn#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:domain>
